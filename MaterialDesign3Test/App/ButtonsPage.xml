<MDElement
  orientation="Vertical"
  overflow="Scroll"
  alignItemsHor="Center"
  alignItemsVer="Start"
  paddingBottom="m: 0, d: 50"
  
  backgroundColor="MDTheme::Get()->SurfaceVariant">

  <Element
    width="100%"
    height="m:0, d: 40"
    alignItemsHor="Start">
    <MDIconButton
      element="mBack"
      name="_s(arrow_back)"
      color="MDTheme::Get()->OnBackground">
    </MDIconButton>
  </Element>

  <MDScaffold
    marginTop="m:0, d: 50"
    width="m: 100%, d: 500"
    height="m: 100%, d: 700"
    paddingBottom="40dp"
    overflow="Scroll"
    orientation="Vertical">
    <MDAppBar
      alignItemsHor="Start">
      <MDAppBarTitle
        text="Buttons">
      </MDAppBarTitle>
    </MDAppBar>
    <Element
      width="100%"
      orientation="Vertical">

      <MDText 
        text="MDIconButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
      <Element
        marginTop="10dp">
        <MDIconButton
          name="_s(menu)"
          color="MDTheme::Get()->PrimaryColor">
        </MDIconButton>
      </Element>

      <MDText
        marginTop="40dp"
        text="MDTextButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
      <Element
        marginTop="10dp">
         <MDTextButton
          label="_s(Button)"
          icon='_s(check)'>
        </MDTextButton>
        <MDTextButton
          marginLeft="20dp"
          label="_s(Button)">
        </MDTextButton>
      </Element>

      <MDText
        marginTop="40dp"
        text="MDOutlinedButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
      <Element
        marginTop="10dp">

         <MDOutlinedButton
          label="_s(Button)"
          icon='_s(check)'>
        </MDOutlinedButton>
        <MDOutlinedButton
          marginLeft="20dp"
          label="_s(Button)">
        </MDOutlinedButton>
       
      </Element>

      <MDText
        marginTop="40dp"
        text="MDElevatedButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
      <Element
        marginTop="10dp">
        <MDElevatedButton
          icon="_s(check)"
          label="_s(Button)">
        </MDElevatedButton>
        <MDElevatedButton
          marginLeft="20dp"
          label="_s(Button)">
        </MDElevatedButton>
      </Element>

      <MDText
        marginTop="40dp"
        text="MDFilledButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
      <Element
        marginTop="10dp">
        <MDFilledButton
          icon="_s(check)"
          label="_s(Button)">
        </MDFilledButton>
        <MDFilledButton
          marginLeft="20dp"
          label="_s(Button)">
        </MDFilledButton>
      </Element>

      <MDText
        marginTop="40dp"
        text="MDFloatingActionButton"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
       <Element
        marginTop="10dp">
        <MDFAB
          icon='_s(check)'
          color="EMDFABColor::Surf">
        </MDFAB>

         <MDLargeFAB
          marginLeft="20dp"
          color="EMDFABColor::Tert"
          icon='_s(check)'>
        </MDLargeFAB>

        <MDSmallFAB
          marginLeft="20dp"
          icon='_s(check)'>
        </MDSmallFAB>

        <!-- <MDFloatingActionButtonExtended
          marginLeft="20dp"
          icon='new MDIcon("add", 32, glm::vec4(255))'
          label="_s(Extended FAB)">
        </MDFloatingActionButtonExtended> -->
      </Element>


      <MDText
        marginTop="40dp"
        text="MDFloatingActionButtonExtended"
        fontSize="18dp"
        fontStyle="Bold"
        textColor="MDTheme::Get()->OnSurface">
      </MDText>
       <Element
        marginTop="10dp">
        <MDFloatingActionButtonExtended
          marginLeft="20dp"
          icon='_s(check)'
          color="EMDFABColor::Surf"
          label="_s(Extended FAB)">
        </MDFloatingActionButtonExtended> 
        <MDFloatingActionButtonExtended
          marginLeft="20dp"
          color="EMDFABColor::Tert"
          label="_s(Extended FAB)">
        </MDFloatingActionButtonExtended> 
      </Element>
    </Element>
  </MDScaffold>

</MDElement>