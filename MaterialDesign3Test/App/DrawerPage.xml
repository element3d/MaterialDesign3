<MDElement
  orientation="Vertical"
  overflow="Scroll"
  alignItemsHor="Center"
  alignItemsVer="Start"
  paddingBottom="m :0, d: 50"
  backgroundColor="MDTheme::Get()->SurfaceVariant">


  <Element
    width="100%"
    height="m: 0, d: 40"
    alignItemsHor="Start">
    <MDIconButton
      element="mBack"
      color="MDTheme::Get()->OnSurface"
      name="_s(arrow_back)">
    </MDIconButton>
  </Element>



  <MDScaffold
    element="mScaffold"
    marginTop="m: 0, d: 50"
    width="m: 100%, d: 500"
    height="m: 100%, d: 700"
    orientation="Vertical"
    backgroundColor="all: 255">
    <MDAppBar
      alignItemsHor="Start">
      <MDAppBarAction
        element="mDrawerButton"
        name="_s(menu)">
      </MDAppBarAction>
      <MDAppBarTitle
        text="Drawer">
      </MDAppBarTitle>
    </MDAppBar>
    <Element
      width="100%"
      height="100%"
      orientation="Vertical"
      scaling="Shrink">

     
    </Element>
    <MDDrawer
      element="mDrawer"
      scaffold="mScaffold"
      >
      <MDElement
        width="m: 80%, d: 60%"
        height="100%"
        overflow="Hidden"
        alignItemsVer="Start"
        orientation="Vertical">

        <Element
          width="100%"
          height="30%"
          backgroundColor="MDTheme::Get()->PrimaryColor">

        </Element>
        <Element
          orientation="Vertical"
          width="100%"
          padding="24dp">
          <MDListTile
            element="mItem1"
            leading='"search"'
            title="_s(Search)">
          </MDListTile>
          <MDListTile
            element="mItem2"
            leading='"3d_rotation"'
            title="_s(3D)">
          </MDListTile>
          <MDListTile
            leading='"double_arrow"'
            title="_s(Accelerator)">
          </MDListTile>
          <MDListTile
            leading='"dashboard"'
            title="_s(Dashboard)">
          </MDListTile>
          <MDDivider>
          </MDDivider>
          <MDListTile
            leading='"logout"'
            title="_s(Logout)">
          </MDListTile>
        </Element>

      </MDElement>
    </MDDrawer>
  </MDScaffold>

</MDElement>