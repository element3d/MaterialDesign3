<MDInkWell
  width="150dp"
  aspectRatio="1"
  margin="10dp"
  orientation="Vertical"
  borderRadius='glm::vec4(e3::Dim("20dp"))'
 
  backgroundColor="MDTheme::Get()->Surface">
  <MDIcon
    element="mIcon"
    name="_s(web_asset)"
    width="36dp"
    aspectRatio="1"
    color="MDTheme::Get()->PrimaryColor">
  </MDIcon>
  <MDText
    element="mTitle"
    text="App bar"
    textColor="MDTheme::Get()->PrimaryColor"
    fontSize="14dp"
    fontStyle="Bold">
  </MDText>
</MDInkWell>